<footer id="footer">
  <div class="footer-left">
    &copy;
    <% var startYear = (theme.copyright && theme.copyright.start_year) ? theme.copyright.start_year : new Date().getFullYear() %>
    <% var endYear = (theme.copyright && theme.copyright.end_year) ? theme.copyright.end_year : new Date().getFullYear() %>
    <%= startYear >= endYear ? endYear : startYear + "-" + endYear %>
    <%= config.author || config.title %>
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var link in theme.social_links) { %>
          <li><a href="<%- url_for(theme.social_links[link]) %>"><i class="fab fa-<%= link %>"></i></a></li>
        <% } %>
      </ul>
    </nav>
  </div>
  <% if (
    theme.creative_common
    && theme.creative_common.license
    && theme.creative_common.version
  ) { %>
    <% var license = theme.creative_common.license %>
    <% var version = theme.creative_common.version %>
    <% var license_path = `${license}/${version}` %>
    <div class="creative_common">
      <a href="https://creativecommons.org/licenses/<%- license_path %>/">
        <img src="https://licensebuttons.net/l/<%- license_path %>/80x15.png" />
      </a>
    </div>
  <% } %>
</footer>
